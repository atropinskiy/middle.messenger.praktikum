.profile-table{
  width: 510px;
  border-spacing: 0;
  box-sizing: border-box;
  margin-top: 50px;
  font-size: 13px;
}



.profile-table td {
  border-bottom: 1px solid #ccc;
  height: 33px;
}

.profile-table tr:last-child td {
  border-bottom: none;
}

.name-label {
  margin-top: 21px;
  font-weight: 600;
}

.profile-save-button {
  width: 280px;
  margin-top: 72px;
}

.profile-input {
  border: none;
  width: 50%;
  display: flex;
  text-align: right;
}
