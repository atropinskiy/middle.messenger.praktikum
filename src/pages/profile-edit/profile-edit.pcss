.editList {
  width: 510px;
  margin: 0;
  margin-top: 50px;
  padding: 0;
  height: 350px;
}

.editList-item {
  padding: 10px;
  border-bottom: 1px solid #cccccc; /* Разделитель между элементами */
}

.editList-item:last-child {
  border-bottom: none; /* Убираем границу у последнего элемента */
}

.editListLabel {
  font-weight: 500;
  font-size: 13px;
}
